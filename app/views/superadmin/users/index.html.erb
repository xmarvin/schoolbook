<h1>Listing users</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Name</th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.name %></td>

    <td><%= link_to 'Edit', edit_superadmin_user_path(user) %></td>
    <td><%= link_to 'Destroy', [:superadmin,user], confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>